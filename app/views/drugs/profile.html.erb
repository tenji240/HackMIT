<%= @drug_profile["drugRecordOutput"] %>
<div class="row-fluid">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-body">
        <h3 class="text-center">Now Showing <%= @drug_profile["drugRecordOutput"]["DrugName"] %></h3>
      </div>
    </div>
  </div>
</div>

<div class="row-fluid">
  <div class="col-md-8">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Other Names</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @drug_profile["drugRecordOutput"]["DrugSynonyms"]["Name"].each do |item| %>
          <li><%= item[0] %></li>
        <% end %>
        <ul>
      </div>
    </div>
  </div>
  <div class="col-md-4">
  </div>
</div>

<hr>
<%= link_to 'Home', drugs_path, class: 'btn btn-info' %>
